{
  "plugins": [
    ["@semantic-release/commit-analyzer", {
      "preset": "eslint",
      "releaseRules": [
      {
        "subject": "no-release:",
        "release": false
      },
      {
        "subject": "!no-release:*",
        "release": "major"
      }
      ],
    "parserOpts": {
      "noteKeywords": [
        "BREAKING CHANGE",
        "BREAKING CHANGES"
      ]
    }
    }],
    ["@semantic-release/release-notes-generator", {
      "preset": "eslint"
    }],
    ["@semantic-release/npm"],
    ["@semantic-release/github"]
  ]
}
